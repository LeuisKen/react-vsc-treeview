{
    "name": "react-vsc-treeview-example",
    "displayName": "React VSC TreeView Example",
    "description": "An example extension that shows how to use react vsc treeview",
    "publisher": "baidu",
    "version": "0.0.1",
    "license": "MIT",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "views": {
            "test": [
                {
                    "id": "baidu.tree.example",
                    "name": "用户面板"
                }
            ]
        }
    },
    "scripts": {
        "clean": "rimraf dist/",
        "compile": "yarn run clean && webpack --mode production --info-verbosity verbose",
        "watch": "yarn run clean && webpack --mode none --watch --info-verbosity verbose"
    },
    "devDependencies": {
        "@types/react": "^17.0.0",
        "@types/vscode": "^1.52.0",
        "rimraf": "^3.0.2",
        "ts-loader": "^8.0.12",
        "typescript": "^4.1.3",
        "webpack": "^4.42.0",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {
        "react": "^17.0.1",
        "react-vsc-treeview": "*"
    }
}
